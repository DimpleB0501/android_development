<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.imagesnestedurl">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CycleTrek"></activity>
        <activity android:name=".Snorkeling" />
        <activity android:name=".Skiing" />
        <activity android:name=".Zorbing" />
        <activity android:name=".Rapelling" />
        <activity android:name=".RiverRafting" />
        <activity android:name=".RockClimbing" />
        <activity android:name=".ScubaDiving" />
        <activity android:name=".Skydiving" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>